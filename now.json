{
  "version": 2,
  "name": "loyalty",
  "builds": [{
    "src": "next.config.js",
    "use": "@now/next"
  }],
  "routes": [{
      "src": "/_next/static/(.*)",
      "dest": "/_next/static/$1"
    },
    {
      "src": "/_next/(.*)",
      "dest": "/_next/$1"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}