{
  "version": 2,
  "name": "loyalty",
  "builds": [{
      "src": "next.config.js",
      "use": "@now/next"
    },
    {
      "src": "public/**",
      "use": "@now/static"
    }
  ],
  "routes": [{
      "src": "/_next/static/(.*)",
      "dest": "/_next/static/$1"
    },
    {
      "src": "/_next/(.*)",
      "dest": "/_next/$1"
    },
    {
      "src": "/images/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000,immutable"
      },
      "dest": "/$1"
    },
    {
      "src": "/public/(.*)",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}